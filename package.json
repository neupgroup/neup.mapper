{
  "name": "@neupgroup/mapper",
  "description": "Neup.Mapper core library for schema and mapping utilities",
  "version": "1.7.17",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "create-migration": "dist/cli/create-migration.js",
    "create-connection": "dist/cli/create-connection.js",
    "update-connection": "dist/cli/update-connection.js",
    "delete-connection": "dist/cli/delete-connection.js",
    "migrate": "dist/cli/migrate.js",
    "migrate-up": "dist/cli/migrate.js",
    "migrate-down": "dist/cli/migrate.js",
    "create-schemas": "dist/cli/create-schemas.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p ./tsconfig.json && chmod +x dist/cli/*.js",
    "create-migration": "npx tsx src/cli/create-migration.ts",
    "create-connection": "npx tsx src/cli/create-connection.ts",
    "update-connection": "npx tsx src/cli/update-connection.ts",
    "delete-connection": "npx tsx src/cli/delete-connection.ts",
    "create-mapper-config": "npx tsx src/cli/create-mapper-config.ts",
    "migrate": "npx tsx src/cli/migrate.ts",
    "migrate-up": "npx tsx src/cli/migrate.ts up",
    "migrate-down": "npx tsx src/cli/migrate.ts down",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "firebase-admin": "^13.6.1",
    "mongodb": "^7.0.0",
    "mysql2": "^3.16.2",
    "pg": "^8.18.0",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2"
  }
}